{
	"name": "instagres-element",
	"version": "0.0.1",
	"keywords": [
		"instagres",
		"custom-element",
		"web-component",
		"database",
		"postgres"
	],
	"type": "module",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist/",
		"package.json"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "tsc --noEmit && vite build",
		"test": "vitest --passWithNoTests",
		"test:ci": "vitest run --passWithNoTests",
		"tsc": "tsc"
	},
	"devDependencies": {
		"@types/node": "catalog:",
		"@types/react": "^19.2.7",
		"@vitest/coverage-v8": "3.0.9",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-solid": "^2.11.10",
		"vitest": "catalog:"
	},
	"peerDependencies": {
		"vite": "^6.0.0 || ^7.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"solid-element": "^1.9.1",
		"solid-js": "^1.9.10"
	}
}
