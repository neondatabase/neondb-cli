{
	"name": "neon-init",
	"version": "0.11.0",
	"description": "Initialize Neon projects",
	"keywords": [
		"neon",
		"database",
		"postgres",
		"init"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neondatabase/neondb-cli.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Neon",
		"url": "https://neon.com"
	},
	"type": "module",
	"main": "dist/index.js",
	"bin": {
